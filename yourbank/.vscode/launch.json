    "version": "0.2.0",
    "configurations": [
      {
        "type": "pwa-node",
        "request": "launch",
        "name": "Launch Program",
        "runtimeArgs": ["dev"],
        "outputCapture": "std",
        "runtimeExecutable": "yarn",
        "skipFiles": ["<node_internals>/**"]
      },
      {
        "type": "pwa-node",
        "request": "launch",
        "name": "Test",
        "runtimeArgs": ["test"],
        "outputCapture": "std",
        "runtimeExecutable": "yarn",
        "env": {"NODE_ENV":"test"},
        "skipFiles": ["<node_internals>/**"]
      }
    ]
  }